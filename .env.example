# Rename this file to .env.local and fill in your values

# NextAuth.js Environment Variables
# Required: Secret for JWT signing
NEXTAUTH_SECRET=your_super_secret_string_here
NEXTAUTH_URL=http://localhost:3000

# Spotify OAuth (auto-detected with AUTH_ prefix)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# OpenAI API (if you're using it)
OPENAI_API_KEY=your_openai_api_key

# Gemini AI (if you're using it)
GEMINI_API_KEY=your_gemini_api_key
